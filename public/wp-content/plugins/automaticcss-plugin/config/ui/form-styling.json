{
	"$schema": "screen.schema.json",
	"id": "form-styling",
	"title": "Form Styling",
	"type": "screen-container",
	"content": [
		{
			"id": "forms-message",
			"type": "section-container",
			"title": "Form Framework",
			"content": [
				{
					"id": "forms-message",
					"type": "message",
					"text": "ACSS gives you the ability to style entire forms with a single utility class. Use '.form--light' or '.form--dark' on your form element and then configure your styling below."
				}
			]
		},
		{
			"id": "option-forms",
			"title": "Load Forms",
			"type": "toggle",
			"style": "primary",
			"default": "on"
		},
		{
			"id": "forms-style-tabs",
