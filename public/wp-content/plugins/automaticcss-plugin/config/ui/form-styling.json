							"id": "label-styling",
							"title": "Label",
							"type": "accordion-container",
							"content": [
								{
									"id": "form-label-text-styles-section",
									"title": "Text",
									"type": "section-container",
									"content": [
										{
											"id": "f-label-size",
											"title": "Label Font Size",
											"type": "text",
											"default": "--text-m"
