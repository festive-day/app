							"content": [
								{
									"id": "f-help-size",
									"title": "Help Font Size",
									"type": "text",
									"default": "--text-s"
								},
								{
									"id": "f-help-line-height",
									"title": "Line Height",
									"type": "number",
									"default": 1.4
								},
								{
									"id": "f-help-text-weight",
									"title": "Font Weight",
									"type": "select",
									"options": [
										"",
										"100",
										"200",
										"300",
										"400",
										"500",
										"600",
										"700",
										"800",
										"900",
										"inherit"
