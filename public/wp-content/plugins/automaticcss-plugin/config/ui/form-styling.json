												"100",
												"200",
												"300",
												"400",
												"500",
												"600",
												"700",
												"800",
												"900",
												"inherit"
											],
											"default": "",
											"validation": {
												"required": false
											}
										},
										{
											"id": "f-label-text-transform",
											"title": "Text Transform",
											"type": "select",
											"default": "none",
											"options": [
